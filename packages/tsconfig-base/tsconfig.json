  "compilerOptions": {
    "jsx": "react",
    "target": "es5",
    "esModuleInterop": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "downlevelIteration": true,
    "lib": [
      "es6",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "declaration": true
  }
